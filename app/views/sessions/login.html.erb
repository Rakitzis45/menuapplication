<h2>Login</h2>
    <%= form_with model:@user, local: true do |f|  %>
        <% if session[:error_message]%>
            <li id="login_error"><%= session[:error_message] %><br></li>
        <% end%>
        <%= f.label :email %>
        <%= f.text_field :email%>
        <br>
        <%= f.label :password %>
        <%= f.password_field :password %>
        <%= f.submit "Login" %>
    <% end%>

<%= link_to "Login via Google Authentication", '/auth/google_oauth2' %>